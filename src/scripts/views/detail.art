{{each Info}}
<div class="detail-content">
        <a class="back" href="#" onclick="javascript: window.history.back();return false;"><i class="back">《</i></a>
    <div class="download-header">
        <div class="logo-img"></div>
        <div class="info-wrapper">
          <div class="title">猫眼</div>
          <div class="content">0 元看电影，天天领现金</div>
        </div>
        <div class="link-btn">立即领取</div>
    </div>


    <div class="poster-bg">
        <div class="blur-bg"></div>
        <div class="bg-666"></div>
        <div class="detail box-flex">
            <div class="poster">
                <img src="{{$value.img}}">
            </div>
            <div class="content flex">
                <div class="title middle line-ellipsis">{{$value.nm}}</div>
                <div class="title-en-name line-ellipsis">{{$value.enm}}</div>
                <div class="score line-ellipsis">
                        {{$value.sc}}    
                    <span class="snum">{{$value.snum}}人评</span>
                </div>
                <div class="type line-ellipsis">
                    <span>{{$value.cat}}</span>
                    
                        <div class="type-group">
                            
                            <img style="width: 26px;" class="imax" src="data:image/png;base64,">
                            
                        </div>
                    
                </div>
                <div class="src line-ellipsis">{{$value.src}}/{{$value.dur}}分钟</div>
                <div class="pubDesc line-ellipsis">{{$value.pubDesc}}</div>
            </div>
        </div>
    </div>

